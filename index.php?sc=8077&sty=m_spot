function checkOdh() {
	var frm = document.predpovedi;
	if(parseInt(frm.odh.value) > parseInt(frm.doh.value)) frm.odh.value = frm.doh.value;
}
function checkDoh() {
	var frm = document.predpovedi;
	if(parseInt(frm.doh.value) < parseInt(frm.odh.value)) frm.doh.value = frm.odh.value;
}

