<script language="JavaScript" type="text/javascript">
//<![CDATA[

function menuSwitch(id,content_id) {
    $(".wgfcst-wgtab_on").removeClass('wgfcst-wgtab_on').addClass('wgfcst-wgtab_off');
    var content_id = content_id || id.substr(0,id.length-4); 
    //alert(id+'/'+content_id);
    $("#"+id).removeClass('wgfcst-wgtab_off').addClass('wgfcst-wgtab_on');
    $(".m_menu_content").hide();
    var content_div;
    if(content_id) content_div = $("#"+content_id);
    if(!content_div.length) { // pokud zadny takovy obsah pro tuto polozku menu neni tak zobraz spot menu
        $("#m_menu").show(); 
    }
    else {
        content_div.show(); 
    }
    if(id=='m_spot_tab'||id=='m_menu_tab'||id=='m_favo_tab'||id=='m_news_tab')tipSwitch(1);else tipSwitch(0);
    if(id=='m_search_tab') {
        $("#spot_search_res").show(); 
    }
    else{
        $("#spot_search_res").hide();
    }
    if(id!='m_spot_tab')removeWGSbanner();
    
}
function removeWGSbanner() {
	$("#WGstationbanner").remove();
}
function tipSwitch(x){
	if(x==1)$("#wgtip_div").show();else $("#wgtip_div").hide();
}
function menuSwitchC(nazev) {
    menuSwitch(nazev+'_tab');
}

menuSwitchC('m_spot');
function wgSubmit(x) {
	frm=document.predpovedi;
	if(x=='spotfav'){wgSc(frm.spotcislofav.value, 'm_favo');}
        else if(x=='spotlast'){wgSc(frm.spotcislofav_last.value,'m_favo');}
	else if(x=='page'){wgPi(frm.pageid.value);}
	else if(x=='spot'){wgSc(frm.id_spot.value,'m_menu');}
}
function wgSc(x,sty) {
	if(x<=0){window.alert('Select some spot!');return;}
        var open = 'index.php?sc='+x;
        if(sty) {
            open += '&sty=' + sty;
        }
        window.open(open,'_self');
}
function wgPi(x) {
	if(x=='0'){window.alert('Select some set!');return;}
        var open = 'index.php?pi='+x;
        window.open(open,'_self');
}

function wgAjaxSearch() {
	frm=document.predpovedi;
	var glcvs = frm.gl_cvselect.value
	$(function(){
	    $("#spot_search_res").prepend("<div class='loading'>Loading... <img style='vertical-align: middle;'src='img/loading.gif'><\/div>");
		$("#spot_search_res").load("ajax/ajax_spot_search.php",
		   {gl_cvselect: glcvs,
		   type: "main",
		   fullsearch: frm.fullsearch.value,
		   nicksearch: frm.nicksearch.value,
		   ajaxSubmitFunction: "wgAjaxSearch"}
		   ,function() {WgUtil.wgUserInfoWindow();}
		);			
	});
}
function wgAjaxSaveOptions() {
	frm=document.predpovedi;
	$(function(){
		$("#setoptions_message").load("ajax/ajax_action.php",
		   {
		   q: "setoptions",
                   wj: frm.wj.value,
                   tj: frm.tj.value,
                   waj: frm.waj.value,
                   odh: frm.odh.value,
                   doh: frm.doh.value,
                   fhours: frm.fhours.value,
                   wrap: frm.wrap.value,
                   vt: frm.vt.value
                   },
                   function() { setTimeout(function() { $("#setoptions_message").empty(); }, 3000);}
                   
		);			
	});
}
function wgAjaxNewSearch() {
	frm=document.predpovedi;
	frm.gl_cvselect.value="";
	wgAjaxSearch();
}

//]]>
</script>
<script language="JavaScript" type="text/javascript">
//<![CDATA[
function showFcst(id) {
        wgSc(id);
}
function pridejSpot(id) {
	frm=document.predpovedi;
	var glcvs = frm.gl_cvselect.value;
	if(!id) return;
	$(function(){
		$("#spot_search_res").prepend("<div class='loading'>Adding... <img style='vertical-align: middle;'src='img/loading.gif'><\/div>");
		$("#spot_search_res").load("ajax/ajax_spot_search.php",
		   {gl_cvselect: glcvs,
		   type: "main",
		   fullsearch: frm.fullsearch.value,
		   nicksearch: frm.nicksearch.value,
		   akce: "new",
		   spotid: id,
		   ajaxSubmitFunction: "wgAjaxSearch"}
		);	
	});

}
        glarr = new Array();

        function wgVarEncode(aktform) {
        var tmp = '';
        var first = true;
        for(var i = 0; i < glarr.length; i++) {
        if(glarr[i] == null) continue;
        if(!first) tmp += ';';
        first = false;
        tmp += ''+i+'/'+glarr[i][0]+'/'+glarr[i][1]+'/'+glarr[i][2]+'/'+glarr[i][3]+'/'+glarr[i][4];
        }
        aktform.gl_cvselect.value = tmp;
        }
